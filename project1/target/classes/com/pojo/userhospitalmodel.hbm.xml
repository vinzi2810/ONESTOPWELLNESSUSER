<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.pojo.userhospitalmodel" table = "userhosmap">
      
      <meta attribute = "class-description">
         This class contains the login details. 
      </meta>
      
      <id name = "tableid" type = "int" column = "tableid"> 
        <generator class="native"></generator>     
      </id>
      
     <property name = "UserID" column = "UserID" type = "string"/>
     <property name = "hosid" column = "hosid" type = "string"/>
   </class>
</hibernate-mapping>